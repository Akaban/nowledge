             <Item.Image size='tiny' circular src={event.hostPhotoURL} />\n                    <Item.Content>\n                        <Item.Header content={event.title} />\n                        <Item.Description>\n                            Hosted by {event.hostedBy}\n                        </Item.Description>\n                        {event.isCancelled && (\n                            <Label\n                                style={{top: '-40px'}}\n                                ribbon='right'\n                                color='red'\n                                content='This event has been cancelled'\n                            />\n                        )\n                        }\n                    </Item.Content>\n                </Item.Group>\n            </Segment>\n            <Segment>\n                <span>\n                    <Icon name='clock' /> {format(event.date, 'MMMM d, yyyy h:mm a')}\n                    <Icon name='marker' /> {event.venue}\n                </span>\n            </Segment>\n            <Segment secondary>\n                <List horizontal>\n                    {event.attendees.map(attendee => (\n                        <EventListAttendee key={attendee.id} attendee={attendee}/>\n                    ))}\n                </List>\n\n            </Segment>\n            <Segment clearing>\n                <span>{event.description}</span>\n                <Button as={Link} to={`/events/${event.id}`} color='teal' floated='right' content='View'/>\n                <Button \n                    onClick={() => deleteEventInFirestore(event.id) } \n                    color='red'\n                    floated='right'\n                    content='Delete'\n                    />\n            </Segment>\n        </Segment.Group>\n    )\n}","/home/bryce/projet_tournesol/revents/src/features/events/eventDashboard/EventListAttendee.jsx",[],["574","575"],"/home/bryce/projet_tournesol/revents/src/features/events/eventForm/EventForm.jsx",["576","577","578","579"],"import cuid from 'cuid';\nimport { Formik, Form} from 'formik';\nimport React, { useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Link, Redirect } from 'react-router-dom';\nimport { Button, Confirm, Header, Segment} from 'semantic-ui-react';\nimport {createEvent, listenToEvents, updateEvent} from '../eventActions'\nimport * as Yup from \"yup\"\nimport MyTextInput from '../../../app/common/form/MyTextInput';\nimport MyTextArea from '../../../app/common/form/MyTextArea';\nimport MySelectInput from '../../../app/common/form/MySelectInput';\nimport MyDateInput from '../../../app/common/form/MyDateInput';\nimport { categoryData } from '../../../app/api/categoryOptions';\nimport useFirestoreDoc from '../../../app/hooks/useFirestoreDoc';\nimport { addEventFirestore, cancelEventToggle, listenToEventFromFirestore, updateEventInFirestore } from '../../../app/firestore/firestoreService';\nimport LoadingComponent from '../../../app/layout/LoadingComponents';\nimport { toast } from 'react-toastify';\n\n\nexport default function EventForm({\n        setFormOpen, setEvents, match, history}) {\n\n    const dispatch = useDispatch();\n    const [loadingCancel, setLoadingCancel] = useState(false)\n    const [confirmOpen, setConfirmOpen] = useState(false)\n    const selectedEvent = useSelector(state => state.event.events.find(e => e.id === match.params.id));\n    const {loading, error} = useSelector(state => state.async)\n\n    const initialValues = selectedEvent ?? {\n        title: '',\n        category: '',\n        description: '',\n        city: '',\n        venue: '',\n        date: '',\n        isCancelled: ''\n    }\n\n    const validationSchema = Yup.object({\n        title: Yup.string().required('You must provide a title'),\n        category: Yup.string().required('You must provide a category'),\n        city: Yup.string().required(),\n        description: Yup.string().required(),\n        venue: Yup.string().required(),\n        date: Yup.string().required(),\n    })\n\n    async function handleCancelToggle(event) {\n        setConfirmOpen(false);\n        setLoadingCancel(true);\n        try {\n            await cancelEventToggle(event)\n        } catch (error) {\n            setLoadingCancel(true)\n            toast.error(error.message)\n        }\n    }\n\n    useFirestoreDoc({\n        shouldExecute: !!match.params.id,\n        query: () => listenToEventFromFirestore(match.params.id),\n        data: event => dispatch(listenToEvents([event])),\n        deps: [match.params.id, dispatch]\n    })\n\n    if (loading) return <LoadingComponent content='Loading event...'/>\n\n    if (error) return <Redirect to='/error' />\n\n    return (\n        <Segment clearing>\n            <Formik\n                initialValues={initialValues}\n                validationSchema={validationSchema}\n                onSubmit={async (values, {setSubmitting}) => {\n                try {\n                    selectedEvent\n                    ? await updateEventInFirestore(values)\n                    : await addEventFirestore(values)\n                        history.push('/events');\n                } catch(error) {\n                    toast.error(error.message)\n                    setSubmitting(false)\n                }\n                }}\n            >\n            {({isSubmitting, dirty, isValid}) => (\n            <Form className='ui form'>\n                <Header content='Event Details' sub color='teal'/>\n                <MyTextInput name='title' placeholder='Event title'/>\n                <MySelectInput name='category' placeholder='Event category' options={categoryData}/>\n                <MyTextArea name='description' placeholder='Event description' rows={3}/>\n                <Header content='Event Location Details' sub color='teal'/>\n                <MyTextInput name='venue' placeholder='Event venue'/>\n                <MyTextInput name='city' placeholder='Event city'/>\n                <MyDateInput\n                    name='date'\n                    placeholderText='Event date'\n                    showTimeSelect\n                    timeCaption='time'\n                    dateFormat='MMMM d, yyyy h:mm a'\n                    />\n               { selectedEvent && \n                <Button\n                    type='button'\n                    floated='left'\n                    color={selectedEvent.isCancelled ? 'green' : 'red'}\n                    content={selectedEvent.isCancelled ? 'Reactivate event' : 'Cancel event'} \n                    onClick={() => setConfirmOpen(true)}\n                    />}\n                <Button\n                    loading={isSubmitting}\n                    disabled={!isValid || !dirty || isSubmitting}\n                    type='submit'\n                    floated='right'\n                    positive\n                    content='Submit' />\n                <Button\n                    disabled={isSubmitting}\n                    as={Link}\n                    to='/events'\n                    type='submit'\n                    floated='right'\n                    content='Cancel' />\n            </Form>\n            )}\n            </Formik>\n            <Confirm\n                content={selectedEvent.isCancelled ? 'This will reactivate the event - are you sure?' :\n                    'This will cancel the event - are you sure?'}\n                open={confirmOpen}\n                onCancel={() => setConfirmOpen(false)}\n                onConfirm={() => handleCancelToggle(selectedEvent) }\n            />\n\n        </Segment>\n    )\n}","/home/bryce/projet_tournesol/revents/src/app/api/sampleData.js",[],"/home/bryce/projet_tournesol/revents/src/features/home/HomePage.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/events/eventsDetailed/EventDetailedPage.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/nav/SignedOutMenu.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/nav/SignedInMenu.jsx",["580"],"\nimport React from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Link, useHistory } from 'react-router-dom';\nimport { toast } from 'react-toastify';\nimport { Dropdown, Image, Menu } from 'semantic-ui-react';\nimport { signOutFirebase } from '../../app/firestore/firebaseService';\n\nexport default function SignedInMenu() {\n\n    const {currentUserProfile} = useSelector(state => state.profile)\n    const history = useHistory();\n\n    async function handleSignOut() {\n        try {\n            history.push('/')\n            await signOutFirebase()\n        } catch (error) {\n            toast.error(error.message)\n        }\n    }\n    return (\n<Menu.Item position='right'>\n    <Image avatar spaced='right' src={currentUserProfile.photoURL || '/assets/user.png'} />\n    <Dropdown pointing='top left' text={currentUserProfile.displayName}>\n        <Dropdown.Menu>\n            <Dropdown.Item as={Link} to='/createEvent' text='Create Event' icon='plus' />\n            <Dropdown.Item as={Link} to={`/profile/${currentUserProfile.id}`} text='My profile' icon='user' />\n            <Dropdown.Item as={Link} to='/account' text='My account' icon='settings' />\n            <Dropdown.Item onClick={handleSignOut} text='Sign out' icon='power' />\n        </Dropdown.Menu>\n    </Dropdown>\n</Menu.Item>\n    )\n}","/home/bryce/projet_tournesol/revents/src/features/events/eventsDetailed/EventDetailedInfo.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/events/eventsDetailed/EventDetailedChat.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/events/eventsDetailed/EventDetailedHeader.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/events/eventsDetailed/EventDetailedSidebar.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/store/configureStore.js",[],"/home/bryce/projet_tournesol/revents/src/features/sandbox/testReducer.js",[],"/home/bryce/projet_tournesol/revents/src/features/sandbox/Sandbox.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/store/rootReducer.js",[],"/home/bryce/projet_tournesol/revents/src/features/events/eventreducer.js",[],"/home/bryce/projet_tournesol/revents/src/features/events/eventConstants.js",[],"/home/bryce/projet_tournesol/revents/src/features/events/eventActions.js",[],"/home/bryce/projet_tournesol/revents/src/app/layout/ScrollToTop.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/common/form/MyTextInput.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/common/form/MyTextArea.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/api/categoryOptions.js",[],"/home/bryce/projet_tournesol/revents/src/app/common/form/MySelectInput.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/common/form/MyDateInput.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/common/modals/modalReducer.js",[],"/home/bryce/projet_tournesol/revents/src/app/common/modals/ModalManager.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/sandbox/TestModal.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/common/modals/ModalWrapper.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/auth/LoginForm.jsx",["581"],"import { Form, Formik} from 'formik'\nimport React from 'react'\nimport ModalWrapper from '../../app/common/modals/ModalWrapper'\nimport * as Yup from 'yup'\nimport MyTextInput from '../../app/common/form/MyTextInput'\nimport { Button, Divider, Label } from 'semantic-ui-react'\nimport { useDispatch } from 'react-redux'\nimport { signInUser } from './authActions'\nimport { closeModal } from '../../app/common/modals/modalReducer'\nimport { signInWithEmail } from '../../app/firestore/firebaseService'\nimport SocialLogin from './SocialLogin'\n\nexport default function LoginForm() {\n    const dispatch = useDispatch();\n    return (<ModalWrapper size='mini' header='Sign in to Re-vents'>\n    <Formik\n        initialValues ={{email: '', password: ''}}\n        validationSchema={Yup.object({\n            email: Yup.string().required().email(),\n            password: Yup.string().required()\n        })}\n        onSubmit={async (values, {setSubmitting, setErrors}) => {\n            try {\n                await signInWithEmail(values)\n                setSubmitting(false)\n                dispatch(closeModal());\n            } catch (error) {\n               setErrors({auth: 'Problem with username or password'})\n               setSubmitting(false)\n               console.log(error) \n            }\n        }}\n    >\n        {({isSubmitting, isValid, dirty, errors}) => (\n            <Form className='ui form'>\n               <MyTextInput name='email' placeholder='Email' />\n               <MyTextInput name='password' placeholder='Password' />\n               {errors.auth && <Label basic color='red' style={{marginBottom: 10}} content={errors.auth} /> }\n               <Button\n                   loading={isSubmitting}\n                   disabled={!isValid || !dirty || isSubmitting}\n                   type='submit'\n                   fluid\n                   size='large'\n                   color='teal'\n                   content='login'\n                 />\n                 <Divider horizontal>\n                     Or\n                 </Divider>\n                 <SocialLogin/>\n            </Form>\n            )}\n\n    </Formik>\n    </ModalWrapper>)\n}","/home/bryce/projet_tournesol/revents/src/features/auth/authReducer.js",[],"/home/bryce/projet_tournesol/revents/src/features/auth/authConstants.js",[],"/home/bryce/projet_tournesol/revents/src/features/auth/authActions.js",[],"/home/bryce/projet_tournesol/revents/src/app/async/asyncReducer.js",[],"/home/bryce/projet_tournesol/revents/src/app/common/util/util.js",[],"/home/bryce/projet_tournesol/revents/src/app/api/mockApi.js",[],"/home/bryce/projet_tournesol/revents/src/app/layout/LoadingComponents.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/events/eventDashboard/EventListItemPlaceholder.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/events/eventDashboard/EventFilters.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/firestore/firestoreService.js",[],"/home/bryce/projet_tournesol/revents/src/app/config/firebase.js",[],"/home/bryce/projet_tournesol/revents/src/app/hooks/useFirestoreCollection.js",[],"/home/bryce/projet_tournesol/revents/src/app/hooks/useFirestoreDoc.js",[],"/home/bryce/projet_tournesol/revents/src/app/common/errors/ErrorComponent.jsx",[],"/home/bryce/projet_tournesol/revents/src/app/firestore/firebaseService.js",[],"/home/bryce/projet_tournesol/revents/src/features/auth/RegisterForm.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/auth/SocialLogin.js",[],"/home/bryce/projet_tournesol/revents/src/features/auth/AccountPage.jsx",["582"],"import { Form, Formik } from 'formik'\nimport React from 'react'\nimport { useSelector } from 'react-redux'\nimport { Button, Header, Label, Segment } from 'semantic-ui-react'\nimport * as Yup from 'yup'\nimport MyTextInput from '../../app/common/form/MyTextInput'\nimport { updateUserPassword } from '../../app/firestore/firebaseService'\n\nexport default function AccountPage() {\n    // const {currentUser} = useSelector(state => state.auth)\n\n    return (\n        <Segment>\n            <Header dividing size='large' content='Account' />\n            <>\n                <Header color='teal' sub content='Change password' />\n                <p>Use this form to change your password</p>\n                <Formik\n                    initialValues={{newPassword1: '', newPassword2: ''}}\n                    validationSchema={\n                        Yup.object({\n                            newPassword1: Yup.string().required('Password is required'),\n                            newPassword2: Yup.string().oneOf([Yup.ref('newPassword1')])\n                        })}\n                    onSubmit={async (values, {setSubmitting, setErrors}) => {\n                        try {\n                            await updateUserPassword(values)\n                        } catch (error) {\n                            setErrors({auth: error.message})\n                        } finally {\n                            setSubmitting(false)\n                        }\n                    }}\n                 >\n                {({errors, isSubmitting, isValid, dirty}) => (\n                <Form className='ui form'>\n                    <MyTextInput name='newPassword1' type='password' plaeholder='New Password' />\n                    <MyTextInput name='newPassword2' type='password' plaeholder='Confirm Password' />\n                    {errors.auth && <Label basic color='red' style={{marginBottom: 10}} content={errors.auth} /> }\n                    <Button \n                        style={{display: 'block'}}\n                        type='submit'\n                        disabled={!isValid || isSubmitting || !dirty}\n                        size='large'\n                        positive\n                        content='Update password'\n                    />\n\n                </Form>\n\n                )}\n                </Formik>\n                </>\n        </Segment>\n    )\n}","/home/bryce/projet_tournesol/revents/src/features/profiles/ProfilePage/ProfilePage.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/profiles/ProfilePage/ProfileHeader.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/profiles/ProfilePage/ProfileContent.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/profiles/profileReducer.js",[],"/home/bryce/projet_tournesol/revents/src/features/profiles/profileConstants.js",[],"/home/bryce/projet_tournesol/revents/src/features/profiles/profileActions.js",[],"/home/bryce/projet_tournesol/revents/src/features/profiles/ProfilePage/AboutTab.jsx",[],"/home/bryce/projet_tournesol/revents/src/features/profiles/ProfilePage/ProfileForm.jsx",[],["583","584"],"/home/bryce/projet_tournesol/revents/src/third/pdfhighlighter/App.js",[],"/home/bryce/projet_tournesol/revents/src/third/pdfhighlighter/Spinner.js",["585"],"import React from \"react\";\n\nimport \"./style/Spinner.css\";\n\nexport default () => {\n  return (\n    <div\n      style={{\n        display: \"flex\",\n        alignItems: \"center\",\n        height: \"100vh\"\n      }}\n    >\n      <div className=\"sk-fading-circle\">\n        <div className=\"sk-circle1 sk-circle\" />\n        <div className=\"sk-circle2 sk-circle\" />\n        <div className=\"sk-circle3 sk-circle\" />\n        <div className=\"sk-circle4 sk-circle\" />\n        <div className=\"sk-circle5 sk-circle\" />\n        <div className=\"sk-circle6 sk-circle\" />\n        <div className=\"sk-circle7 sk-circle\" />\n        <div className=\"sk-circle8 sk-circle\" />\n        <div className=\"sk-circle9 sk-circle\" />\n        <div className=\"sk-circle10 sk-circle\" />\n        <div className=\"sk-circle11 sk-circle\" />\n        <div className=\"sk-circle12 sk-circle\" />\n      </div>\n    </div>\n  );\n};\n","/home/bryce/projet_tournesol/revents/src/third/pdfhighlighter/Sidebar.js",[],"/home/bryce/projet_tournesol/revents/src/third/pdfhighlighter/test-highlights.js",[],"/home/bryce/projet_tournesol/nowledge/src/index.js",[],["586","587"],"/home/bryce/projet_tournesol/nowledge/src/reportWebVitals.js",[],"/home/bryce/projet_tournesol/nowledge/src/app/store/configureStore.js",[],"/home/bryce/projet_tournesol/nowledge/src/app/layout/ScrollToTop.jsx",[],"/home/bryce/projet_tournesol/nowledge/src/app/layout/App.jsx",["588","589","590","591"],"import { Route, useLocation } from \"react-router-dom\";\nimport { Container } from \"semantic-ui-react\";\nimport BooksDashboard from \"../../features/books/booksDashboard/BooksDashboard\";\nimport HomePage from \"../../features/home/HomePage\";\nimport NavBar from \"../../features/nav/NavBar\";\nimport ModalManager from \"../common/modals/ModalManager\";\nimport {ToastContainer} from 'react-toastify'\nimport ErrorComponent f